@import "../../../common/vendor.wxss";

.shezhi {
  padding: 40rpx;
}
.action-row {
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
  justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}
.arrow-right {
  position: absolute;
  width: 50rpx;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto;
}