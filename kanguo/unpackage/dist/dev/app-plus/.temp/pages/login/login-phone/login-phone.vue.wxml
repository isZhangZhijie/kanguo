<template name="0a459e56">
  <view class="_view data-v-6ffd8d90 container">
    <view class="_view data-v-6ffd8d90 login-page-top">
      <view class="_view data-v-6ffd8d90 top-img">
        <navigator class="_navigator data-v-6ffd8d90 go-back" hover-class="none" open-type="navigateBack" delta="1">
          <image src="../../../static/images/common_return@3x.png" mode="widthFix" alt class="_image data-v-6ffd8d90"></image>
        </navigator>
        <image class="_image data-v-6ffd8d90" src="../../../static/images/login_PhoneBG@3x.png" mode="widthFix"></image>
      </view>
      <view class="_view data-v-6ffd8d90 login-input phone-num">
        <image src="../../../static/images/login_signPhone@3x.png" mode="aspectFit" class="_image data-v-6ffd8d90"></image>
        <input type="number" focus="true" value="{{inputClearValue}}" bindinput="handleProxy" placeholder="请输入手机号码" data-eventid="{{'ijd-0'}}" data-comkey="{{$k}}" class="_input data-v-6ffd8d90" />
        <view class="_view data-v-6ffd8d90 uni-icon uni-icon-clear" wx:if="{{showClearIcon}}" bindtap="handleProxy" data-eventid="{{'0jk-1'}}" data-comkey="{{$k}}"></view>
      </view>
      <view class="_view data-v-6ffd8d90 login-input phone-password">
        <image src="../../../static/images/login_signLock@3x.png" mode="aspectFit" class="_image data-v-6ffd8d90"></image>
        <input value="{{password}}" placeholder="请输入登录密码" bindinput="handleProxy" password="{{showPassword}}" data-eventid="{{'TXx-2'}}" data-comkey="{{$k}}" class="_input data-v-6ffd8d90" />
        <view class="_view data-v-6ffd8d90 uni-icon uni-icon-eye {{[!showPassword ? 'uni-active' : '']}}" bindtap="handleProxy" data-eventid="{{'DO5-3'}}" data-comkey="{{$k}}"></view>
      </view>
      <view class="_view data-v-6ffd8d90 login-btn" bindtap="handleProxy" data-eventid="{{'SMB-4'}}" data-comkey="{{$k}}">登 录</view>
      <view class="_view data-v-6ffd8d90 action-row">
        <navigator url="../../reg/phone/phone" hover-class="none" class="_navigator data-v-6ffd8d90"> 快速注册 </navigator>
        <navigator url="../forget/phone/phone" hover-class="none" class="_navigator data-v-6ffd8d90"> 忘记密码 </navigator>
      </view>
    </view>
    <view class="_view data-v-6ffd8d90 other-login-way" style=" {{('top:' + (positionTop + 'px') + ';')}}">
      <text class="_text data-v-6ffd8d90">其他方式登录</text>
      <view class="_view data-v-6ffd8d90 other-way-list">
        <image bindtap="handleProxy" class="_image data-v-6ffd8d90 other-login" src="../../../static/images/login_button_WeixinEnter@3x.png" data-eventid="{{'kkR-5'}}" data-comkey="{{$k}}"></image>
      </view>
    </view>
  </view>
</template>